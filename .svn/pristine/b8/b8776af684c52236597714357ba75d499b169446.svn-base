/* pages/newIndex/newIndex.wxss */
page{
  position: relative;
  width: 100%;
  height: 100vh;
  padding-bottom: 150rpx;
  overflow-y: auto;
}
.myFirst{
  left: -20rpx;
}
.box{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
}

.box view.cu-bar {
  margin-top: 20rpx; 
}
.cu-bar.tabbar .action .cuIcon-cu-image image {
  width: 50rpx;
  height: 45rpx;
  display: inline-block;
}
.myMenu {
  position: absolute;
  top: 350rpx;
  width: -webkit-fill-available;
}
.container{
  width: 100%;
  height: 500rpx;
  margin: 30rpx 0;
}
.menuImg{
  width:60rpx;
  height:60rpx;
  /* padding-top: 30rpx; */
}
.nav .cu-item {
  /* height: 90rpx; */
  display: inline-block;
  line-height: 50rpx;
  margin: 0 10rpx;
  margin-top: 20rpx;
  padding: 0 20rpx;
}
.nav .cu-item.cur {
  border-bottom: 0rpx solid;
}
